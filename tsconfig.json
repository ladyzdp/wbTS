
{
   "compilerOptions": {
    "target": "es5",
    "outDir": "./built",
    "allowJs": true, 
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false
  },
  "include": [
      "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}